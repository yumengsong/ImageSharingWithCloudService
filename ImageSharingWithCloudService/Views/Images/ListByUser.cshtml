@model SelectList
@{
    ViewBag.Title = "List By User";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@{
    Html.BeginForm("DoListByUser", "Images", FormMethod.Get);
}

<fieldset>
    <legend>Choose a user </legend>

    <p>
        User: @Html.DropDownList("Id", Model)
    </p>
    <p>
        <input type="submit" value="Query" />
    </p>
</fieldset>

@ViewBag.Message

@{ Html.EndForm(); }




