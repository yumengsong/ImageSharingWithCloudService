@model SelectList
@{
    ViewBag.Title = "List By Tag";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@{
    Html.BeginForm("DoListByTag", "Images", FormMethod.Get);
}

<fieldset>
    <legend>Choose a tag </legend>
    @*
        @Html.AntiForgeryToken()
    *@
    <p>
        Tag: @Html.DropDownList("Id", Model)
    </p>
    <p>
        <input type="submit" value="Query" />
    </p>
</fieldset>

@ViewBag.Message

@{ Html.EndForm(); }



