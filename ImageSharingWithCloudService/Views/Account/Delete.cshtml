@model List<ImageSharingWithCloudService.Models.SelectItemView>
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@{  Html.BeginForm("Delete", "Account", FormMethod.Post);}

<fieldset>
    <legend>Deactivate/Reactivate Users</legend>
    @Html.AntiForgeryToken()

    <table class="gridTabel" border="1">
        <tr>
            <th>Deactivate</th>
            <th>User</th>
        </tr>
        @for (var i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>
                    @Html.HiddenFor(us => us[i].Userid)
                    @Html.CheckBoxFor(us => us[i].Checked)
                </td>
                <td>
                    @Html.DisplayFor(us => us[i].Name)
                </td>
            </tr>
        }

    </table>

    <p>
        <input type="submit" value="Submit" />
    </p>
</fieldset>

@ViewBag.Message
@{  Html.EndForm();}








